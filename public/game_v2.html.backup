    function getChipColor(color) {
        const colors = {
            'blue': '#4285f4',
            'red': '#ea4335',
            'green': '#34a853',
            'yellow': '#fbbc04',
            'purple': '#9c27b0',
            'orange': '#ff9800',
            'white': '#ffffff'
        };
        return colors[color] || '#666';
    }

    function getChipStyle(color) {
        const baseStyle = {
            background: getChipColor(color),
            color: color === 'white' ? '#000' : '#fff',
            border: color === 'white' ? '2px solid #333' : '1px solid rgba(255,255,255,0.2)'
        };
        return baseStyle;
    }

